#define _CRT_SECURE_NO_WARNINGS

#include <stdio.h>
#define MIN(a,b) a<b?a:b

int dp[1000001];

int main()
{
    int n = 0;

    scanf("%d", &n);

    for (int i = 1; i <= n; i++) {
        dp[i] = i;
    }

    for (int i = 2; i <= n; i++)
    {
        dp[i] = dp[i - 1] + 1;

        if (i % 2 == 0)
        {
            dp[i] = (MIN(dp[i], dp[i / 2] + 1));
        }
        if (i % 3 == 0)
        {
            dp[i] = (MIN(dp[i], dp[i / 3] + 1));
        }
    }

    printf("%d\n", dp[n]);

    while (n != 0) {
        printf("%d ", n);

        if (dp[n] == dp[n - 1] + 1)
        {
            n = n - 1;
        }

        else if (n % 2 == 0 && dp[n] == dp[n / 2] + 1)
        {
            n = n / 2;
        }

        else if (n % 3 == 0 && dp[n] == dp[n / 3] + 1)
        {
            n = n / 3;
        }
    }

    return 0;
}
